  "repository": {
    "type": "git",
    "url": "git+https://github.com/MKGaru/rpio-define.git"
  },
  "keywords": [
    "gpio",
    "raspberrypi",
    "rpio"
  ],
  "author": "MKGaru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MKGaru/rpio-define/issues"
  },
  "homepage": "https://github.com/MKGaru/rpio-define#readme",
  "devDependencies": {
    "@types/rpio": "^2.4.2",
    "rpio": "^2.4.2",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "rpio": "*"
  }
}
